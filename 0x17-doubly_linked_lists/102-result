#include <stdio.h>

int main() {
  int i, j, max_palindrome = 0;

  for (i = 100; i <= 999; i++) {
    for (j = 100; j <= 999; j++) {
      int product = i * j;

      if (is_palindrome(product)) {
        max_palindrome = product;
      }
    }
  }

  printf("%d\n", max_palindrome);

  return 0;
}

int is_palindrome(int number) {
  char str[10];

  sprintf(str, "%d", number);

  int len = strlen(str);

  for (int i = 0; i < len / 2; i++) {
    if (str[i] != str[len - 1 - i]) {
      return 0;
    }
  }

  return 1;
}
